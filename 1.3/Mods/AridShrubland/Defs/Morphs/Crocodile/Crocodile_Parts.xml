<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef Name="CrocodilePart" ParentName="PawnmorphPart" Abstract="true">
		<classInfluence>PMEA_CrocodileMorph</classInfluence>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="CrocodilePart">
		<defName>PMEA_CrocodileSnout</defName>
		<label>crocodile snout</label>
		<description>A snout graces [PAWN_nameDef]'s face, like that of a crocodile.</description>
		<mutationTale>GrewMuzzleMisc</mutationTale>
		<parts>
			<li>Jaw</li>
		</parts>
		<graphics>
			<Muzzle>Parts/Crocodile/Crocodile_Face</Muzzle>
		</graphics>
		<stages>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<capMods>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.10</offset>
					</li>
					<li>
						<capacity>Eating</capacity>
						<offset>0.1</offset>
					</li>
				</capMods>
				<graphics>
					<Muzzle>Parts/None/None</Muzzle>
				</graphics>
			</li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<capMods>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.40</offset>
					</li>
					<li>
						<capacity>Eating</capacity>
						<offset>0.1</offset>
					</li>
				</capMods>
				<graphics>
					<Muzzle>Parts/Partials/PartialMuzzle/TinyMuzzle</Muzzle>
				</graphics>
				<minSeverity>0.1</minSeverity>
			</li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<capMods>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.40</offset>
					</li>
					<li>
						<capacity>Eating</capacity>
						<offset>0.1</offset>
					</li>
				</capMods>
				<graphics>
					<Muzzle>Parts/Partials/PartialMuzzle/PartialSnout</Muzzle>
				</graphics>
				<minSeverity>0.3</minSeverity>
			</li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<capMods>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.20</offset>
					</li>
					<li>
						<capacity>Eating</capacity>
						<offset>0.40</offset>
					</li>
				</capMods>
				<graphics>
					<Muzzle>Parts/Crocodile/Crocodile_Face</Muzzle>
				</graphics>
				<minSeverity>0.5</minSeverity>
			</li>
			<li>
				<label>adapted</label>
				<minSeverity>1</minSeverity>
				<capMods>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.1</offset>
					</li>
					<li>
						<capacity>Eating</capacity>
						<offset>0.40</offset>
					</li>
				</capMods>
			</li>
		</stages>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<capacities>
							<li>Bite</li>
						</capacities>
						<power>10</power>
						<cooldownTime>2.6</cooldownTime>
						<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
						<chanceFactor>0.7</chanceFactor>
					</li>
				</tools>
			</li>
			<li Class="Pawnmorph.Hediffs.CompProperties_MutationSeverityAdjust">
				<severityPerDay>0.25</severityPerDay>
			</li>
		</comps>
		<categories>
			<li>Major</li>
		</categories>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="TortoisePart">
		<defName>PMEA_CrocodileEar</defName>
		<label>earhole</label>
		<description>A fleshy membrane has grown over the earhole, reducing sensitivity to higher pitches but granting the ability to detect stomping feet three rooms away.</description>
		<parts>
			<li>Ear</li>
		</parts>
		<graphics>
			<LeftEar>Parts/None/None</LeftEar>
			<LeftEarBehind>Parts/None/None</LeftEarBehind>
			<RightEar>Parts/None/None</RightEar>
			<RightEarBehindHead>Parts/None/None</RightEarBehindHead>
		</graphics>
		<mutationTale>EarsChangedMisc</mutationTale>
		<stages>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<description>A fleshy membrane has grown over [PAWN_nameDef]'s earhole, reducing [PAWN_possessive] sensitivity to higher pitch sounds but granting [PAWN_objective] the ability to detect footfalls from three rooms away.</description>
				<partEfficiencyOffset>0.05</partEfficiencyOffset>
				<statOffsets>
					<NegotiationAbility>-0.02</NegotiationAbility>
					<TradePriceImprovement>-0.02</TradePriceImprovement>
					<SocialImpact>-0.02</SocialImpact>
				</statOffsets>
			</li>
		</stages>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="CrocodilePart">
