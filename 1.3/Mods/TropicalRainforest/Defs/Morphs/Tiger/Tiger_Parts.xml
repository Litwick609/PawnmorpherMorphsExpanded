<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation= "https://raw.githubusercontent.com/Tachyonite/Pawnmorpher/dev/Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="PantherineJawPart">
		<classInfluence>PMAE_TigerMorph</classInfluence>
		<defName>PMAE_TigerMuzzle</defName>
		<label>tiger muzzle</label>
		<graphics>
			<Muzzle>Parts/Tiger/Tiger_Muzzle</Muzzle>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<description>TODO Tiger. The strange shape makes speaking very difficult.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<description>TODO Tiger. Adapting to it has made speaking less frustrating.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>paragon</stageKey>
				<values>
					<description>TODO Tiger. With further adaptation, [PAWN_nameDef] has learned to speak normally.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="PantherineEarPart">
		<classInfluence>PMAE_TigerMorph</classInfluence>
		<defName>PMAE_TigerEar</defName>
		<label>tiger ear</label>
		<graphics>
			<LeftEar>Parts/Tiger/Tiger_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/None/None</LeftEarBehind>
			<RightEar>Parts/Tiger/Tiger_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/None/None</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>TODO Tiger</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="PantherineTailPart">
		<classInfluence>PMAE_TigerMorph</classInfluence>
		<defName>PMAE_TigerTail</defName>
		<label>tiger tail</label>
		<graphics>
			<Tail>Parts/Tiger/Tiger_Tail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>TODO Tiger</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
</Defs>
